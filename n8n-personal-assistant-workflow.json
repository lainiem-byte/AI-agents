      "parameters": {
        "jsCode": "// Find calls happening in the next 30-40 minutes that haven't had brief sent\nconst calls = $json.rows || [];\nconst now = new Date();\nconst in30Min = new Date(now.getTime() + 30*60*1000);\nconst in40Min = new Date(now.getTime() + 40*60*1000);\n\nconst upcomingCalls = calls.filter(call => {\n  if (!call['Call Date/Time'] || call['Brief Sent'] === true) return false;\n  \n  const callTime = new Date(call['Call Date/Time']);\n  return callTime >= in30Min && callTime <= in40Min;\n});\n\nreturn upcomingCalls.map(call => ({\n  call_id: call['Call ID'],\n  lead_id: call['Lead ID'],\n  call_time: call['Call Date/Time'],\n  business_name: call['Business Name'],\n  contact_name: call['Contact Name'],\n  market: call['Market'],\n  industry: call['Industry'],\n  audit_score: call['Audit Score'],\n  aesthetic_score: call['Aesthetic Score'],\n  labor_leakage_score: call['Labor Leakage Score'],\n  psychic_opening: call['Psychic Opening'],\n  logic_hammer: call['Logic Hammer'],\n  architectural_path: call['Architectural Path'],\n  scarcity_close: call['Scarcity Close'],\n  key_objections: call['Key Objections'],\n  proof_assets: call['Proof Assets']\n}));"
      },
